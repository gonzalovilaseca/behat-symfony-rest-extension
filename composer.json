{
  "name": "gvf/behat-symfony-rest-extension",
  "description": "A Behat extension for testing Symfony Restful APIs",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "behat/behat": "^3.5",
    "nahid/jsonq": "^5.2",
    "phpunit/phpunit": "^9.3",
    "symfony/framework-bundle": "^5.0",
    "friends-of-behat/symfony-extension": "^2.1",
    "symfony/browser-kit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Gvf\\SymfonyRestExtension\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0"
    }
  }
}
